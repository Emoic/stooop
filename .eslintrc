{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true,
    "jquery": true
  },
  "plugins": [
    "import",
    "mocha",
    "chai-expect"
  ],
  "extends": "airbnb-base",
  "rules": {
    "comma-dangle": 0,
    "consistent-return": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "no-shadow": 0,
    "no-console": 0,
    "no-plusplus": 0,
    "array-callback-return": 0,
    "import/no-extraneous-dependencies": 0,
    "mocha/no-exclusive-tests": "error",
    "chai-expect/terminating-properties": 0
  }
}
